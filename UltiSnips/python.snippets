snippet debug "pydebug"
print('***************')
print(${1})
print('***************')
endsnippet

snippet forb "check value in array"
k = 0
for i in ${1}:
	print(${2})
	if k == 10:
		break
	k += 1
endsnippet

snippet pr "print"
print(${1})
endsnippet

snippet iff "iff"
if ${1} ${2} ${3}:
	${4}
endsnippet

